      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "color": {
      "type": "string",
      "description": "Background color",
      "default": "lightblue"
    },
    "opacity": {
      "type": "number",
      "description": "Opacity",
      "default": 1
    },
    "width": {
      "type": "integer",
      "default": 100
    },
    "height": {
      "type": "integer",
      "default": 100
    }
  },
  "methods": {
    "setRange": {
      "description": "Select a range in the rangeSelector",
      "parameters": [
        {
          "name": "selectionIdx",
          "type": "integer",
          "description": "The index of the range to select"
        }
      ]
    },
    "setColor": {
      "description": "Sets the background color.",
      "parameters": [
        {
          "name": "newColor",
          "type": "string",
          "description": "The new background color"
        }
      ],
      "body": "this.color = newColor;"
    },
    "getColor": {
      "returnType": "string",
      "description": "Returns the background color.",
      "body": "return this.color;"
    }
  },
  "events": {
    "onClick": {
      "description": "Called when the user clicks the Colored Box."
    }
  },
  "dataBindings": {
    "myDataSource": {
      "feeds": [
        {
          "id": "dimensions",
          "description": "The dimensions feed",
          "type": "dimension"
        },
        {
          "id": "measures",
          "description": "The measures feed",
          "type": "mainStructureMember"
        }
      ]
    }
  }
